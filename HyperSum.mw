<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;####################################################                     HyperSum                    ##         Developed by Madhukara Kekulandara      ##                                                 ## Evaluate sums using hypergeometric functions    ##                                                 ## exp - summation expression (without sigma)      ## var - variable of the expression                ## lower - lower bound of the summation            ## upper - upper bound of the summation            ##                                                 ####################################################HyperSum := proc(exp,var,lower,upper)  local ratio,Z,nmrtr,dnmntr,facts,L1,L2,newvar,newexp,f;  print(exp);      #Shifting the varible to make initial term is 0  newvar := var + lower;   #Sustitute the expression according to the shifted variable  newexp := subs(var=newvar,exp);    #Get the ratio of two consecutive terms.  ratio := subs(var=var+1,newexp)/newexp;  print(ratio);  ratio := simplify(expand(ratio));  Z:= 1;    #Separate the numerator and denominator from the ratio.
  nmrtr := expand(numer(ratio));
  dnmntr := expand(denom(ratio));  if ( not type(nmrtr,polynom(anything,var)) )
     then ERROR(`Not a hypergeometric series`);
  fi:  if ( not type(dnmntr,polynom(anything,var)) )
     then ERROR(`Not a hypergeometric series`);
  fi:  Z:= Z * lcoeff(nmrtr,var);    #Solve the numerator for 'var'.
  facts := [solve(nmrtr=0,var)];    #Multiply the factors with (-1).  L1 := map(proc(x)normal(-x);end,facts);
  L1 := [ op(L1), 1 ];  Z:= Z / lcoeff(dnmntr,var);    #Solve the denominator for 'var'.
  facts := [solve(dnmntr=0,var)];
    #Multiply the factors with (-1).  L2 := map(proc(x)normal(-x);end,facts);      #Form the hypergeomatric function  print(hypergeom(L1,L2,Z));    #solve the hypergeomatric function  eval(simplify(hypergeom(L1,L2,Z),hypergeom),[n=upper]);end proc:</Text-field>
</Input>
